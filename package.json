{
  "name": "args-command-router",
  "version": "3.0.0-alpha.3",
  "description": "Nodejs command-line argument router",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "play": "npm run build; node ./play/index.js",
    "build": "tsc",
    "test": "cross-env TS_NODE_FILES=true mocha --exit --require ts-node/register --reporter spec --colors test/*.test.ts test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "publish": "np"
  },
  "keywords": [
    "args",
    "argument",
    "router",
    "tty",
    "command-line"
  ],
  "author": "Sahand Javid <sahand.javid@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/SHND/args-command-router.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.19.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^6.2.3",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "args-command-parser": "^1.1.11",
    "command-line-usage": "^6.1.1"
  }
}
